    "Zutat 1":"1 EL Weißweinessig",
    "Zutat 2":"1/2 Zwiebeln",
    "Zutat 3":"1 Knoblauchzehe",
    "Zutat 4":"4 Currywürste",
    "Zutat 5":"200ml Cola",
    "Zutat 6":"Zucker, Curry, Salz und Pfeffer",
